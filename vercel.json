{
  "version": 2,
  "buildCommand": null,
  "routes": [
    { "handle": "filesystem" },
    { "src": "/", "dest": "/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Content-Security-Policy", "value": "frame-ancestors *;" },
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    }
  ]
}